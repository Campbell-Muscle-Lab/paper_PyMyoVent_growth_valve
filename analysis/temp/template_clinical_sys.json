{
    "layout":
    {
        "fig_width": 12,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 3,
        "grid_wspace": -0.5
    },
    "x_display":{
        "global_x_field": "subject_type",
        "label": "Subjects",
        "order": ["CD", "CS", "PD", "PS"]
    },
    "formatting":
    {
        "x_label_fontsize": 12,
        "y_label_fontsize": 12,
        "tick_fontsize": 12,
        "legend_fontsize": 10,
        "y_label_pad": 40,
        "x_label_pad":20,
        "legend_bbox_to_anchor": [1.0, 1.05],
        "color_theme_category": "Set2",
        "patch_alpha": 0.5,
        "data_linewidth": 1,
        "max_rows_per_legend":6
    },
    "strip_formatting":
    {
        "jitter": false,
        "dodge":false
    },
    "annotations":
    [
        {
            "panel": 0,
            "type": "text",
            "label": "Aortic Stenosis",
            "x_coord": 0.5,
            "y_rel_coord": 1.2,
            "label_fontsize": 16
        },
