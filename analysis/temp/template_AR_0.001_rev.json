    "panels":
    [
        {
            "column": 1,
            "y_info":
            {
                "label": "Myofilament\n[$\\mathregular{Ca^{2+}}$]\n($\\mathregular{\\mu}$M)",
                "series":
                [
                    {
                        "field": "Ca_cytosol",
                        "scaling_factor": 1e6,
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Half-sarcomere\nlength\n(nm)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "hs_length",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Wall stress\n(kN $\\mathregular{m^{-2}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "hs_stress",
                        "field_label": "Total",
                        "scaling_factor": 1e-3,
                        "style": "envelope"

                    },
                    {
                        "field": "cb_stress",
                        "field_label": "Active",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    },
                    {
                        "field": "int_pas_stress",
                        "field_label": "Passive",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Thin\nfilament",
                "series":
                [
                    {
                        "field": "n_off",
                        "field_label": "N$\\mathregular{_{on}}$",
                        "style": "envelope"

                    },
                    {
                        "field": "n_on",
                        "field_label": "N$\\mathregular{_{off}}$",
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Thick\nfilament",
                "series":
                [
                    {
                        "field": "M_SRX",
                        "field_label": "M$\\mathregular{_{SRX}}$",
                        "style": "envelope"

                    },
                    {
                        "field": "M_DRX",
                        "field_label": "M$\\mathregular{_{DRX}}$",
                        "style": "envelope"
                    },
                    {
                        "field": "M_FG",
                        "field_label":  "M$\\mathregular{_{FG}}$",
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Ventricular\npressure\n(mmHg)",
                "series":
                [
                    {
                        "field": "pressure_ventricle",
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Venous\npressure\n(mmHg)",
                "series":
                [
                    {
                        "field": "pressure_veins",
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Ventricular\ncavity volume\n(liters)",
                "series":
                [
                    {
                        "field": "volume_ventricle",
                        "style": "envelope"
                    }

                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label":"Ventricular\nwall\nthickness\n(mm)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "ventricle_wall_thickness",
                        "style": "envelope",
                        "scaling_factor": 1e3
                    }

                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Arterial\npressure\n(mmHg)",
                "series":
                [
                    {
                        "field": "pressure_arteries",
                        "field_label": "P$\\mathregular{_{arteries}}$",
                        "style": "envelope"
                    },
                    {
                        "field": "baro_b_setpoint",
                        "field_label": "P$\\mathregular{_{set}}$",
                        "field_color": "red"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Heart\nrate\n(bpm)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "heart_rate"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{act}}$\n(M $\\mathregular{s^{-1}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "k_act"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{SERCA}}$\n(M $\\mathregular{s^{-1}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "k_serca"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_1}$\n($\\mathregular{s^{-1}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "k_1"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_3}$\n($\\mathregular{s^{-1}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "k_3"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{on}}$\n($\\mathregular{\\mu}$$\\mathregular{M^{-1}}$$\\mathregular{s^{-1}}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "k_on",
                        "scaling_factor": 1e-8
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "R$\\mathregular{_{arteriolar}}$\n(mmHg $\\mathregular{L^{-1} s}$)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "arterioles_resistance"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "C$\\mathregular{_{veins}}$\n([mmHg]$\\mathregular{^{-1}}$ L)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "veins_compliance"
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label":"Aortic\nregurgitant\nvolume\n(ml)",
                "scaling_type": "close_fit",
                "ticks": [0,46],
                "series":
                [
                    {
                        "field": "aortic_reg_volume_max",
                        "scaling_factor": 1e3
                    }

                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "ATPase to\n myofibrillar\nvolume\n(kJ $\\mathregular{[m^{3} s]^{-1}}$)",
                "series":
                [
                    {
                        "field": "ATPase_to_myo",
                        "field_label": "S$\\mathregular{_{con}}$",
                        "style": "envelope",
                        "scaling_factor": 1e-3
                    },
                    {
                        "field": "gr_concentric_set",
                        "field_label": "S$\\mathregular{_{con,set}}$",
                        "field_color": "red",
                        "scaling_factor": 1e-3
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "G$\\mathregular{_{a,con}}$",
                "ticks": [0,1], 
                "series":
                [
                    {
                        "field": "growth_concentric_g",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "G$\\mathregular{_{c,con}}$\n($\\mathregular{\\times 10^{-3}}$)",
                "series":
                [
                    {
                        "field": "growth_concentric_c",
                        "style": "envelope",
                        "scaling_factor": 1e3
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "Ventricular\nwall volume\n(liters)",
                "series":
                [
                    {
                        "field": "ventricle_wall_volume"
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "Intracellular\npassive stress\n(kN $\\mathregular{m^{-2}}$)",
                "scaling_type": "close_fit",
                "ticks": [-0.05,2.5],  
                "series":
                [
                    {
                        "field": "cpt_int_pas_stress",
                        "field_label": "S$\\mathregular{_{ecc}}$",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    },
                    {
                        "field": "gr_eccentric_set",
                        "field_label": "S$\\mathregular{_{ecc,set}}$",
                        "scaling_factor": 1e-3,
                        "field_color": "red"
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "G$\\mathregular{_{a,ecc}}$",
                "ticks": [0,1], 
                "series":
                [
                    {
                        "field": "growth_eccentric_g",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "G$\\mathregular{_{c,ecc}}$\n($\\mathregular{\\times 10^{-3}}$)",
                "series":
                [
                    {
                        "field": "growth_eccentric_c",
                        "style": "envelope",
                        "scaling_factor": 1e3
                    }
                ]
            }
        },
        {
            "column": 3,
            "y_info":
            {
                "label": "Number\nof\nhalf-sarcomeres\n(thousands)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "n_hs",
                        "scaling_factor": 1e-3
                    }
                ]
            }
        }
    ]
