        {
            "column": 1,
            "y_info":
            {
                "label": "n_hs",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "n_hs",
                        "style": "line"
                    }
                ]
            }
        },
        {
            "column": 1,
            "scaling_type": "close_fit",
            "y_info":
            {
                "label": "Wall\nvolume\n(liters)",
                "series":
                [
                    {
                        "field": "ventricle_wall_volume",
                        "style": "line"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Wall\nthickness\n(m)",
                "series":
                [
                    {
                        "field": "ventricle_wall_thickness",
                        "style": "envelope"
                    }
                ]
            }
