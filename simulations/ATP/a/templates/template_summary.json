{
    "layout":
    {
        "fig_width": 14,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 1,
        "grid_wspace": 1,
        "grid_hspace": 1
    },
    "x_display":{
        "global_x_field": "time",
        "label": "Time (s)"
    },
    "processing":{
        "envelope_rel_width": 0.02
    },
    "formatting":
    {
        "x_label_fontsize": 10,
        "y_label_fontsize": 10,
        "tick_fontsize": 7,
        "legend_fontsize": 6,
        "label_fontsize": 3,
        "y_label_pad": 10,
        "bbox_to_anchor": [1.3, 1],
        "patch_alpha": 0.3
    },
    "panels":
    [
        {
            "column": 1,
            "y_info":
            {
                "label": "Arterial\npressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_arteries",
                        "field_label": "Pressure",
                        "style": "envelope"
                    },
                    {
                        "field": "baro_P_set",
                        "field_label": "Baroreflex setpoint",
                        "style": "line"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Ventricular\npressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_ventricle",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Ventricular\nvolume\n(liters)",
                "series":
                [
                    {
                        "field": "volume_ventricle",
                        "style": "envelope"
                    }
                ]
            }
